   <string notr="true">QMainWindow {
	background-color: #00111F;
	font-family: "Segoe UI", "Roboto", "Helvetica Neue", sans-serif;
}

QLabel {
	color: #FFFFFF;
}

QPushButton {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1, stop: 0 #B66DDB, stop: 1 #8A4BB0);
	border-radius: 24px;
	color: white;
	font-size: 16px;
	font-weight: 500;
	height: 48px;
	padding: 0 24px;
	border: 2px solid #B66DDB;
	transition: all 0.3s ease;
}

QPushButton:hover {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1, stop: 0 #A05CC4, stop: 1 #7A3BA0);
	border: 2px solid #A05CC4;
	box-shadow: 0 4px 8px rgba(182, 109, 219, 0.4);
}

QPushButton:pressed {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1, stop: 0 #8A4BB0, stop: 1 #6A2B90);
	border: 2px solid #8A4BB0;
	transform: translateY(2px);
}

QPushButton:disabled {
	opacity: 0.5;
}

/* Header */
QLabel#headerTitle {
	font-size: 24px;
	font-weight: 600;
	margin: 24px 0;
	text-align: center;
}

/* Back Button */
QPushButton#backButton {
	background-color: #00223F;
	border-radius: 12px;
	font-size: 18px;
	padding: 0 20px;
	min-width: 40px;
}

QPushButton#backButton:hover {
	background-color: #00335F;
	box-shadow: 0 2px 4px rgba(0, 34, 63, 0.4);
}

/* Start Button */
QPushButton#startEmotionButton {
	margin: 0 32px 24px 32px;
}

/* Channel Section */
QWidget[channelSection="true"] {
	background-color: rgba(0, 34, 63, 0.3);
	border-radius: 16px;
	margin: 15px 32px;
	padding: 15px;
}

/* Channel Header */
QLabel[channelHeader="true"] {
	font-size: 20px;
	font-weight: 600;
	color: #4DA1FF;
	margin: 10px 0;
	text-align: center;
}

/* Section Headers */
QLabel[sectionHeader="true"] {
	font-size: 18px;
	font-weight: 600;
	color: #B66DDB;
	margin: 10px 0 5px 0;
}

/* Progress Bars */
QProgressBar {
	border: 2px solid #4DA1FF;
	border-radius: 10px;
	background-color: rgba(0, 34, 63, 0.5);
	margin: 8px 0;
	height: 20px;
	text-align: center;
}

QProgressBar::chunk {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1, stop: 0 #B66DDB, stop: 1 #8A4BB0);
	border-radius: 8px;
}

/* Status Labels */
QLabel[statusLabel="true"] {
	font-size: 16px;
	margin: 5px 0;
	padding: 8px;
	border-radius: 8px;
	background-color: rgba(0, 34, 63, 0.3);
}

/* Data Grids */
QGridLayout[dataGrid="true"] {
	margin: 10px 0;
	spacing: 10px;
}

/* Grid Headers */
QLabel[gridHeader="true"] {
	font-weight: 600;
	font-size: 16px;
	color: #4DA1FF;
	text-align: center;
}

/* Value Labels */
QLabel[valueLabel="true"] {
	font-weight: 600;
	font-size: 18px;
	color: #20C997;
	text-align: center;
	background-color: rgba(0, 34, 63, 0.3);
	border-radius: 8px;
	padding: 8px;
}

/* Scroll Area */
QScrollArea {
	border: none;
	background-color: transparent;
}

QScrollArea QWidget {
	background-color: transparent;
}

/* Status Bar */
QStatusBar {
	background-color: #00111F;
	border: none;
	border-top: 1px solid #4DA1FF;
}

QStatusBar QLabel {
	color: #FFFFFF;
	font-size: 14px;
}</string>
