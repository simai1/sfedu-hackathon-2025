   <string notr="true">QMainWindow {
	background-color: #00111F;
	font-family: "Segoe UI", "Roboto", "Helvetica Neue", sans-serif;
}

QLabel {
	color: #FFFFFF;
}

QPushButton {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1, stop: 0 #B66DDB, stop: 1 #8A4BB0);
	border-radius: 24px;
	color: white;
	font-size: 16px;
	font-weight: 500;
	height: 48px;
	padding: 0 24px;
	border: 2px solid #B66DDB;
	transition: all 0.3s ease;
}

QPushButton:hover {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1, stop: 0 #A05CC4, stop: 1 #7A3BA0);
	border: 2px solid #A05CC4;
	box-shadow: 0 4px 8px rgba(182, 109, 219, 0.4);
}

QPushButton:pressed {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1, stop: 0 #8A4BB0, stop: 1 #6A2B90);
	border: 2px solid #8A4BB0;
	transform: translateY(2px);
}

QPushButton:disabled {
	opacity: 0.5;
}

/* Header */
QLabel#headerTitle {
	font-size: 24px;
	font-weight: 600;
	margin: 24px 0;
	text-align: center;
}

/* Back Button */
QPushButton#backButton {
	background-color: #00223F;
	border-radius: 12px;
	font-size: 18px;
	padding: 0 20px;
	min-width: 40px;
}

QPushButton#backButton:hover {
	background-color: #00335F;
	box-shadow: 0 2px 4px rgba(0, 34, 63, 0.4);
}

/* Start Button */
QPushButton#resistButton {
	margin: 0 32px 24px 32px;
}

/* Data Grid */
QGridLayout#gridLayout {
	margin: 20px;
	spacing: 15px;
}

/* Labels */
QLabel {
	font-size: 16px;
	color: #FFFFFF;
}

/* Value Labels */
QLabel[id$="Value"] {
	font-weight: 600;
	font-size: 18px;
	color: #4DA1FF;
}

/* Quality Labels */
QLabel[id$="Q"] {
	font-weight: 600;
	font-size: 18px;
	border-radius: 8px;
	padding: 8px 16px;
}

QLabel#o1Q[quality="good"], 
QLabel#o2Q[quality="good"], 
QLabel#t3Q[quality="good"], 
QLabel#t4Q[quality="good"] {
	background-color: rgba(32, 201, 151, 0.3);
	color: #20C997;
}

QLabel#o1Q[quality="poor"], 
QLabel#o2Q[quality="poor"], 
QLabel#t3Q[quality="poor"], 
QLabel#t4Q[quality="poor"] {
	background-color: rgba(220, 53, 69, 0.3);
	color: #DC3545;
}

/* Header Labels */
QLabel#label_6, QLabel#label_7 {
	font-weight: 600;
	font-size: 18px;
	color: #B66DDB;
	text-align: center;
}

/* Channel Labels */
QLabel#label, QLabel#label_2, QLabel#label_3, QLabel#label_4 {
	font-weight: 600;
	font-size: 18px;
	color: #4DA1FF;
}

/* Status Bar */
QStatusBar {
	background-color: #00111F;
	border: none;
	border-top: 1px solid #4DA1FF;
}

QStatusBar QLabel {
	color: #FFFFFF;
	font-size: 14px;
}</string>
