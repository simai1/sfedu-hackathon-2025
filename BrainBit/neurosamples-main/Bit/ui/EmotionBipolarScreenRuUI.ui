   <string notr="true">QMainWindow {
	background: #0A0E1A;
	font-family: "Segoe UI", "Roboto", "Helvetica Neue", sans-serif;
}

QLabel {
	color: #E0E0E0;
}

QPushButton {
	background: rgba(77, 161, 255, 0.1);
	border: 1px solid rgba(77, 161, 255, 0.3);
	color: #E0E0E0;
	font-size: 14px;
	font-weight: 500;
	height: 40px;
	padding: 0 20px;
	border-radius: 8px;
	transition: all 0.3s ease;
}

QPushButton:hover {
	background: rgba(77, 161, 255, 0.25);
	border: 1px solid rgba(77, 161, 255, 0.6);
	box-shadow: 0 4px 12px rgba(77, 161, 255, 0.3);
	transform: translateY(-2px);
}

QPushButton:pressed {
	background: rgba(77, 161, 255, 0.15);
	transform: translateY(0px);
	box-shadow: 0 2px 6px rgba(77, 161, 255, 0.2);
}

QPushButton:disabled {
	opacity: 0.4;
	cursor: not-allowed;
}

/* Header */
QLabel#headerTitle {
	font-size: 24px;
	font-weight: 600;
	margin: 24px 0 8px 0;
	text-align: center;
	color: #4DA1FF;
}

/* Back Button */
QPushButton#backButton {
	background: rgba(77, 161, 255, 0.1);
	border: 1px solid rgba(77, 161, 255, 0.3);
	border-radius: 8px;
	font-size: 14px;
	padding: 0 16px;
	min-width: 60px;
	height: 40px;
}

QPushButton#backButton:hover {
	background: rgba(77, 161, 255, 0.2);
	border: 1px solid rgba(77, 161, 255, 0.5);
}

/* Start Button */
QPushButton#startBipolarEmotionButton {
	background: rgba(77, 161, 255, 0.1);
	border: 1px solid rgba(77, 161, 255, 0.3);
	border-radius: 8px;
	font-size: 14px;
	padding: 0 20px;
	min-width: 120px;
	height: 40px;
}

QPushButton#startBipolarEmotionButton:hover {
	background: rgba(77, 161, 255, 0.2);
	border: 1px solid rgba(77, 161, 255, 0.5);
}

/* Section Headers */
QLabel[objectName="sectionHeader"] {
	font-size: 13px;
	font-weight: 500;
	color: #999999;
	margin: 8px 0 4px 0;
}

/* Progress Bar */
QProgressBar#calibrationProgress {
	border: 1px solid rgba(77, 161, 255, 0.2);
	border-radius: 6px;
	background: rgba(77, 161, 255, 0.05);
	margin: 8px 24px;
	height: 8px;
	text-align: center;
}

QProgressBar#calibrationProgress::chunk {
	background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #4DA1FF, stop: 1 #6BB5FF);
	border-radius: 5px;
}

/* Status Labels */
QLabel#artSequenceLabel, QLabel#artBothSidesLabel {
	font-size: 12px;
	margin: 4px 24px;
	padding: 6px 12px;
	border-radius: 6px;
	background: rgba(77, 161, 255, 0.05);
	border: 1px solid rgba(77, 161, 255, 0.1);
	color: #CCCCCC;
}

/* Data Grids */
QGridLayout {
	margin: 8px 24px;
	spacing: 8px;
}

/* Grid Headers */
QLabel[property*="labelType=gridHeader"] {
	font-weight: 500;
	font-size: 12px;
	color: #999999;
	text-align: center;
}

/* Value Labels */
QLabel[property*="labelType=value"] {
	font-weight: 600;
	font-size: 14px;
	color: #4DA1FF;
	text-align: center;
	background: rgba(77, 161, 255, 0.08);
	border: 1px solid rgba(77, 161, 255, 0.15);
	border-radius: 6px;
	padding: 6px 10px;
}

/* Status Bar */
QStatusBar {
	background: #0A0E1A;
	border: none;
	border-top: 1px solid rgba(77, 161, 255, 0.15);
}

QStatusBar QLabel {
	color: #999999;
	font-size: 12px;
}</string>
